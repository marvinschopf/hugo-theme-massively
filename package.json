{
	"name": "hugo-theme-massively",
	"version": "6.0.0",
	"description": "HTML5 UP theme Massively for Hugo",
	"scripts": {
		"hugo-dev": "cd exampleSite && hugo server --themesDir ../..",
		"style:fix": "prettier --write ./ ."
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/marvinschopf/hugo-theme-massively.git"
	},
	"keywords": [
		"hugo",
		"hugo-theme",
		"theme",
		"html5up",
		"massively"
	],
	"author": {
		"name": "Marvin Schopf",
		"email": "marvin@schopf.biz",
		"url": "https://www.marvinschopf.com"
	},
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/marvinschopf/hugo-theme-massively/issues"
	},
	"homepage": "https://github.com/marvinschopf/hugo-theme-massively#readme",
	"devDependencies": {
		"prettier": "2.2.1",
		"prettier-plugin-go-template": "0.0.10"
	},
	"prettier": {
		"useTabs": true,
		"tabWidth": 4,
		"semi": true,
		"singleQuote": false,
		"overrides": [
			{
				"files": [
					"*.html"
				],
				"options": {
					"parser": "go-template"
				}
			}
		]
	}
}
